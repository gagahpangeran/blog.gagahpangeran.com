{
  "name": "gpr-blog",
  "private": true,
  "description": "GPR's blog built using Gatsby and Markdown",
  "version": "0.1.0",
  "author": "GPR <gpr@gagahpangeran.com>",
  "dependencies": {
    "@fontsource/fira-code": "^4.2.1",
    "@fontsource/lato": "^4.2.1",
    "@fontsource/rubik": "^4.2.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-catch-links": "^2.10.0",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^2.2.5",
    "gatsby-remark-copy-linked-files": "^2.4.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.5.1",
    "gatsby-remark-prismjs": "^3.7.0",
    "gatsby-remark-responsive-iframe": "^2.5.0",
    "gatsby-remark-smartypants": "^2.4.0",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-transformer-remark": "^2.10.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "lodash.kebabcase": "^4.1.1",
    "prismjs": "^1.23.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.12.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "yarn format-prettier && yarn format-eslint",
    "format-prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "format-eslint": "eslint --fix  \"**/*.{ts,tsx}\"",
    "lint": "yarn lint-prettier && yarn lint-eslint",
    "lint-prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "lint-eslint": "eslint --quiet \"**/*.{ts,tsx}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gagahpangeran/blog.gagahpangeran.com"
  },
  "bugs": {
    "url": "https://github.com/gagahpangeran/blog.gagahpangeran.com/issues"
  }
}
